<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btnCachaca"
        android:layout_width="@dimen/tamanhoLarguraFotoProduto"
        android:layout_height="@dimen/tamanhoAlturaFotoProduto"
        android:layout_marginBottom="7dp"
        android:background="@drawable/cachaca_pitu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.103"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.162"
        tools:ignore="DuplicateIds,MissingConstraints" />

    <TextView
        android:id="@+id/txtViewCachaca"
        android:layout_width="@dimen/tamanhoLarguraNomeProdutos"
        android:layout_height="@dimen/tamanhoAlturaNomeProdutos"
        android:layout_marginBottom="25dp"
        android:text="Cachaça"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="@dimen/fonteProdutos"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.102"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.343"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnWhiskey"
        android:layout_width="@dimen/tamanhoLarguraFotoProduto"
        android:layout_height="@dimen/tamanhoAlturaFotoProduto"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="7dp"
        android:background="@drawable/whisky_white_horse"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.525"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.162"
        tools:ignore="DuplicateIds,MissingConstraints" />


    <TextView
        android:id="@+id/txtViewWhisky"
        android:layout_width="@dimen/tamanhoLarguraNomeProdutos"
        android:layout_height="@dimen/tamanhoAlturaNomeProdutos"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="26dp"
        android:text="Whisky"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="@dimen/fonteProdutos"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.554"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.344"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnVodka"
        android:layout_width="@dimen/tamanhoLarguraNomeProdutos"
        android:layout_height="@dimen/tamanhoAlturaFotoProduto"
        android:layout_marginBottom="7dp"
        android:background="@drawable/vodkas_slova"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.884"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.162"
        tools:ignore="DuplicateIds,MissingConstraints" />

    <TextView
        android:id="@+id/txtViewVodka"
        android:layout_width="@dimen/tamanhoLarguraNomeProdutos"
        android:layout_height="@dimen/tamanhoAlturaNomeProdutos"
        android:layout_marginBottom="25dp"
        android:text="Vodka"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="@dimen/fonteProdutos"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.884"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.343"
        tools:ignore="MissingConstraints" />


    <Button
        android:id="@+id/btnCerveja"
        android:layout_width="@dimen/tamanhoLarguraFotoProduto"
        android:layout_height="@dimen/tamanhoAlturaFotoProduto"
        android:layout_marginBottom="10dp"
        android:background="@drawable/cervejas_skol"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.102"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.474"
        tools:ignore="DuplicateIds,MissingConstraints" />

    <TextView
        android:id="@+id/txtViewTequila"
        android:layout_width="@dimen/tamanhoLarguraNomeProdutos"
        android:layout_height="@dimen/tamanhoAlturaNomeProdutos"
        android:text="Cerveja"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="@dimen/fonteProdutos"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.102"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.601"
        tools:ignore="MissingConstraints" />

    <Button

        android:id="@+id/btnPetisco"
        android:layout_width="@dimen/tamanhoLarguraFotoProduto"
        android:layout_height="@dimen/tamanhoAlturaFotoProduto"
        android:layout_marginEnd="20dp"
        android:background="@drawable/petiscos_amedoim_verde"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.525"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.443"
        tools:ignore="DuplicateIds,MissingConstraints" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="@dimen/tamanhoLarguraNomeProdutos"
        android:layout_height="@dimen/tamanhoAlturaNomeProdutos"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="34dp"
        android:text="Petisco"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="@dimen/fonteProdutos"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.559"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.597"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnRefrigerante"
        android:layout_width="@dimen/tamanhoLarguraFotoProduto"
        android:layout_height="@dimen/tamanhoAlturaFotoProduto"
        android:background="@drawable/refrigerante_coca_cola"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.877"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.466"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="@dimen/tamanhoLarguraNomeProdutos"
        android:layout_height="@dimen/tamanhoAlturaNomeProdutos"
        android:layout_marginTop="7dp"
        android:text="Refrigerante"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="@dimen/fonteProdutos"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.877"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.597"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Depósito da Esquina"
        android:textColor="@color/titulos"
        android:textSize="@dimen/tamanhoTitulo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.037"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>